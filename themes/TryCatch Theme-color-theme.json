{
	// Configuración de nombre y colores de la interfaz de usuario
	"name": "TryCatch Theme",
	"colors": {
	  // Configuración de colores de la interfaz de usuario
	  "editor.background": "#232122", // Establece el color de fondo del editor de código.
	  "editor.foreground": "#F2F2F2", // Define el color del texto en el editor de código.
	  "activityBarBadge.foreground": "#EDF2C2", // Color del número de notificaciones en la barra de actividad.
	  "activityBarBadge.background": "#575656", // Color de fondo de los indicadores de actividad.
	  "sideBarTitle.foreground": "#f8f8f8", // Color del título en la barra lateral.
	  "editor.lineHighlightBackground": "#474646", // Color de fondo del resaltado de línea.
	  "editorCursor.foreground": "#F6FFC8", // Color del cursor en el editor.
	  "editor.selectionBackground": "#C0BDBD", // Color de fondo de la selección de texto.
	  "activityBar.background": "#232122", // Color de fondo de la barra de actividad.
	  "activityBar.foreground": "#F2FFAD", // Color del texto en la barra de actividad.
	  "sideBar.background": "#383738", // Color de fondo de la barra lateral.
	  "sideBar.foreground": "#EDEDED", // Color del texto en la barra lateral.
	  "statusBar.background": "#232122", // Color de fondo de la barra de estado.
	  "statusBar.foreground": "#EDF2C2", // Color del texto en la barra de estado.
	  "statusBar.noFolderBackground": "#474646", // Color de fondo de la barra de estado cuando no hay carpeta abierta.
	  "statusBar.noFolderForeground": "#F2F2F2", // Color del texto en la barra de estado cuando no hay carpeta abierta.
	  "titleBar.activeBackground": "#232122", // Color de fondo de la barra de título cuando la ventana está activa.
	  "titleBar.activeForeground": "#F2F2F2", // Color del texto en la barra de título cuando la ventana está activa.
	  "titleBar.inactiveBackground": "#474646", // Color de fondo de la barra de título cuando la ventana no está activa.
	  "titleBar.inactiveForeground": "#B5B5B5", // Color del texto en la barra de título cuando la ventana no está activa.
	  "badge.background": "#647171", // Color de fondo de los distintivos, como los errores en los archivos.
	
	},
	"tokenColors": [
	  // Configuración de estilos para elementos de sintaxis en el código
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#766E6E"
		}
	  },
	  {
		"name": "Function and Storage",
		"scope": ["function", "support.function.builtin", "storage", "storage.type"],
		"settings": {
		  "foreground": "#E7F670"
		}
	  },
	  {
		"name": "Strings",
		"scope": ["string", "string.quoted.single"],
		"settings": {
		  "foreground": "#E89E00"
		}
	  },
	  {
		"name": "Keywords and Types",
		"scope": ["keyword", "keyword.control.flow", "type", "support.type"],
		"settings": {
		  "foreground": "#3CD9E3"
		}
	  },
	  {
		"name": "Type",
		"scope": ["type", "support.type"],
		"settings": {
		  "foreground": "#DF4E4E"
		}
	  }
	],


	}